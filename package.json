{
  "name": "rnflappybird",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone SE (2nd generation)\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev-android": "react-native run-android --variant=devDebug --appIdSuffix dev",
    "dev-android-release-variant": "react-native run-android --variant=devRelease --appIdSuffix dev",
    "dev-android-release": "cd android && ./gradlew clean && ./gradlew assembleDevRelease",
    "staging-android": "react-native run-android --variant=stagingDebug --appIdSuffix staging",
    "staging-android-release-variant": "react-native run-android --variant=stagingRelease --appIdSuffix staging",
    "staging-android-release": "cd android && ./gradlew clean && ./gradlew assembleStagingRelease",
    "production-android": "react-native run-android --variant=productionDebug --appIdSuffix production",
    "production-android-release-variant": "react-native run-android --variant=productionRelease --appIdSuffix production",
    "production-android-release": "cd android && ./gradlew clean && ./gradlew assembleProductionRelease"
  },
  "dependencies": {
    "matter-js": "^0.18.0",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-config": "^1.4.6",
    "react-native-game-engine": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/matter-js": "^0.17.7",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
